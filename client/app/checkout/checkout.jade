.container
	navbar
	.row
		div.col-xs-10.col-xs-offset-1
			div#form-container
				.row
					div#status-buttons.text-center.col-md-8.col-md-offset-2
						.row
							a.col-md-4.col-xs-4(ng-class="{'active': $state.is('checkout.product_check')}")
								.in-circle 
									span.h5 STEP
									br
									spna.h2 01
								p 确认购物清单
									br
									| CHECK ORDER
							a.col-md-4.col-xs-4(ng-class="{'active': $state.is('checkout.shipment_payment')}")
								.in-circle
									span.h5 STEP 
									br
									span.h2 02
									br
								p 付款方式
									br
									| SELECT PAYMENT
							a.col-md-4.col-xs-4(ng-class="{'active': $state.is('checkout.final_confirm')}")
								.in-circle 
									span.h5 STEP 
									br
									span.h2 03
								p 最后确认
									br
									| COMFIRM ORDER
				.row
					form.form-horizontal(name="checkout_form", novalidate)
						div#form-views(ui-view="")
	
